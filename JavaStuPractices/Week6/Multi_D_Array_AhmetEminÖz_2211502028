public class multidArray {
    public static void main(String[] args) {
        //declare and initialize
        int[][] matrix = {{1, 2, 3,}, {4, 5, 6}, {7, 8, 9}};

        //access and display
        for (int i = 0; i < matrix.length; i++) {
            for (int j = 0; j< matrix[i].length; j++){
                    System.out.print(matrix[i][j]+" ");
                }
            System.out.println();
            }
        }
    }
